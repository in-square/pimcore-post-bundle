{
  "name": "in-square/pimcore-post-bundle",
  "description": "Post bundle for Pimcore.",
  "type": "pimcore-bundle",
  "keywords": ["pimcore", "post", "blog", "pimcore-bundle", "pimcore-plugin"],
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/in-square/pimcore-post-bundle",
  "authors": [
    {
      "name": "InSquare",
      "email": "kontakt@insquare.pl",
      "homepage": "https://insquare.pl/"
    }
  ],
  "require": {
    "php": "^8.3",
    "symfony/framework-bundle": "^6.4",
    "symfony/console": "^6.4",
    "pimcore/pimcore": "^11.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.64",
    "phpstan/phpstan": "^1.11"
  },
  "autoload": {
    "psr-4": {
      "InSquare\\PimcorePostBundle\\": "src/"
    }
  },
  "scripts": {
    "phpstan": "phpstan analyse -c phpstan.neon",
    "php-cs": "php-cs-fixer fix --config=.php-cs",
    "php-cs:dry": "php-cs-fixer fix --config=.php-cs --dry-run --diff"
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "InSquare\\PimcorePostBundle"
      ]
    }
  }
}
